import { stringify } from 'qs';
import request from '@/utils/request';

{{#each requests}}
export async function {{functionName}}(params) {
  return request({
    method: '{{method}}',
    url: '{{url}}',
    {{#if params}}
    params: params,
    {{/if}}
    {{#if body}}
    data: params,
    {{/if}}
  })
}
{{/each}}